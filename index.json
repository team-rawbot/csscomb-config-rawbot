{
  "always-semicolon": true,
  "block-indent": "  ",
  "element-case": "lower",
  "eof-newline": true,
  "leading-zero": false,
  "quotes": "single",
  "space-before-colon": "",
  "space-between-declarations": "\n",
  "space-before-opening-brace": " ",
  "space-after-opening-brace": "\n",
  "space-after-selector-delimiter": "\n",
  "space-before-selector-delimiter": "",
  "space-before-closing-brace": "\n",
  "strip-spaces": true,
  "tab-size": true,
  "unitless-zero": true,
  "exclude": [
    ".git/**",
    "node_modules/**"
  ],
  "sort-order": [
    [
      "$extend",
      "$include"
    ],
    [
      "content",
      "quotes",
      "counter-reset",
      "counter-increment"
    ],
    [
      "box-sizing",
      "display",
      "width",
      "min-width",
      "max-width",
      "height",
      "min-height",
      "max-height",
      "margin",
      "margin-top",
      "margin-right",
      "margin-bottom",
      "margin-left",
      "padding",
      "padding-top",
      "padding-right",
      "padding-bottom",
      "padding-left",
      "visibility",
      "float",
      "clear",
      "overflow",
      "overflow-x",
      "overflow-y",
      "-webkit-overflow-scrolling",
      "clip",
      "zoom"
    ],
    [
      "grid",
      "grid-template",
      "grid-template-rows",
      "grid-template-areas",
      "grid-template-columns",
      "grid-area",
      "grid-gap",
      "grid-row",
      "grid-row-start",
      "grid-row-gap",
      "grid-row-end",
      "grid-column",
      "grid-column-start",
      "grid-column-gap",
      "grid-column-end",
      "grid-auto-flow",
      "grid-auto-rows",
      "grid-auto-columns",
      "flex",
      "flex-flow",
      "flex-direction",
      "flex-wrap",
      "flex-grow",
      "flex-shrink",
      "flex-basis",
      "align-items",
      "align-content",
      "align-self",
      "justify-content",
      "order"
    ],
    [
      "position",
      "top",
      "right",
      "bottom",
      "left",
      "z-index"
    ],
    [
      "color",
      "font",
      "font-family",
      "font-size",
      "font-weight",
      "font-style",
      "font-variant",
      "font-size-adjust",
      "font-stretch",
      "font-effect",
      "font-emphasize",
      "font-emphasize-position",
      "font-emphasize-style",
