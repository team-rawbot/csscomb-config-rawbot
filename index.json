      "text-overflow-mode",
      "text-shadow",
      "white-space",
      "vertical-align",
      "letter-spacing",
      "word-spacing",
      "word-wrap",
      "word-break",
      "tab-size",
      "hyphens"
    ],
    [
      "background",
      "background-color",
      "background-image",
      "background-repeat",
      "background-attachment",
